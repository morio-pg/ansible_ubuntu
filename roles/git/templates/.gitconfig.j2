[user]
        name = {{ git.name }}
        email = {{ git.email }}
[core]
        autocrlf = input
        quotepath = false
[diff]
        compactionHeuristic = true
[fetch]
        prune = true
[alias]
        graph = log --graph --date=short --format='%C(yellow)%h%C(reset) %C(magenta)[%ad]%C(reset)%C(auto)%d%C(reset) %s %C(cyan)@%an%C(reset)'
